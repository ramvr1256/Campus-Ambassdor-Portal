            <div class="panel page-form">
              <div class="panel-heading">
                <h2>Create a new task</h2>
              </div>

              <div class="panel-body">
                <div class="form">
                  <form
                    id="uploadForm"
                    enctype="multipart/form-data"
                    action="/project/createproject"
                    method="POST"
                  >
                    <div class="form-group">
                      <label>Title<font color="red">*</font></label>
                      <input
                        type="text"
                        class="form-control"
                        name="title"
                        minlength=3
                        required
                      />
                    </div>
                    <div class="form-group">
                      <label>Whatsapp<font color="red"></font></label>
                      <input
                        type="text"
                        class="form-control"
                        name="whatsapp"
                        required
                      />
                    </div>
                    <div class="form-group">
                      <label>Facebook<font color="red"></font></label>
                      <input
                        type="text"
                        class="form-control"
                        name="facebook"
                        required
                      />
                    </div>
                    <div class="form-group">
                      <label>Twitter<font color="red"></font></label>
                      <input
                        type="text"
                        class="form-control"
                        name="twitter"
                        required
                      />
                    </div>
                    <div class="form-group">
                      <label>Last Date<font color="red">*</font></label>
                      <input
                        type="text"
                        class="form-control"
                        name="lastDate"
                      />
                    </div>
                    <div class="form-group">
                      <label
                        >POINTS<font color="red">*</font></label
                      >
                      <input
                        type="number"
                        class="form-control"
                        name="points"
                        min="1"
                        max="100"
                        required
                      />
                    </div>
                    <div class="form-group">
                      <label>Description<font color="red">*</font></label>
                      <textarea class="form-control"
                        name="description" form="uploadForm" rows="10"> </textarea>
                    </div>
                    <font color="red">*</font> marked fields must be filled.
                    <br />
                    <br />
                   
                    <div class="text-right">
                      <button
                        type="submit"
                        class="btn btn-primary mx-auto form-submit-btn"
                      >
                        Submit
                      </button>
                    </div>
                  </form>
                  <!-- </div>
                  </div> -->
                </div>
              </div>
            </div>
            <!-- CREATE PROJECT -->
